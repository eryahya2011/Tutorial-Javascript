const gameName = new String('Yahya-J');

console.log(gameName[2]);
console.log(gameName.__proto__)


console.log(gameName.length);
console.log(gameName.toUpperCase());
console.log(gameName.charAt(2));   // To find the letter at any place say 2 so in Yahya its h.
console.log(gameName.indexOf('h'));   // To find number of any letter say h in Yahya which is 2. 

const newString = gameName.substring(0, 4)
console.log(newString);

const anotherString = gameName.slice(-5, 4);
console.log(anotherString);

const newStringOne = "    yahya    "
console.log(newStringOne);
